fn a()
    print("a...")
    throw Error("RuntimeError", "foo")
end

fn call_a()
    try
        print("call a()...")
        a()
    catch e : Int # 故意使其匹配不到错误类型
        print("666")
    end
end

try
    print("call call_a()...")
    call_a()
catch e: Str
    print("error is str")
catch e: Error
    print(e.k) # 故意使其报错
end
